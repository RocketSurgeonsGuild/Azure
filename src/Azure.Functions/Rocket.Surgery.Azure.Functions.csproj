<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;net461</TargetFrameworks>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Microsoft.NET.Sdk.Functions" Version="$(Microsoft_NET_Sdk_Functions_Version)" NoWarn="True" />
        <PackageReference Include="Autofac" Version="$(Autofac_Version)" />
        <PackageReference Include="Rocket.Surgery.Extensions.Autofac" Version="$(RSG_DependencyInjection_Version)" />
        <PackageReference Include="Autofac.Extras.CommonServiceLocator" Version="$(Autofac_Extras_CommonServiceLocator_Version)" />
    </ItemGroup>
    <ItemGroup Condition="'$(TargetFramework)'=='netstandard2.0' or '$(TargetFramework)'=='net461'">
        <PackageReference Include="NETStandard.Library" Version="2.0.0" NoWarn="true" PrivateAssets="All" />
    </ItemGroup>
    <ItemGroup Condition="'$(TargetFramework)'=='netstandard1.3' or '$(TargetFramework)'=='net451'">
        <PackageReference Include="NETStandard.Library" Version="1.6.1" NoWarn="true" PrivateAssets="All" />
    </ItemGroup>
</Project>
